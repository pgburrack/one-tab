{
  "manifest_version": 2,

  "name": "oneTab",
  "description": "Manage all open tabs in one tab",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["myscript.js"]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
